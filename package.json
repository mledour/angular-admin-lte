    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "admin-lte": "^2.4.2",
    "angular-loading-page": "^0.4.2",
    "angular-morris-js": "^0.2.1",
    "bootstrap": "^3.3.7",
